{
    "ZOMATO":{
        "CONFIG":{
            "STARTER":{
                "URLS":{
                    "delhi":"https://www.zomato.com/delhi",
                    "bangalore":"https://www.zomato.com/bangalore",
                    "jaipur":"https://www.zomato.com/jaipur",
                    "gurgaon":"https://www.zomato.com/gurgaon",
                    "kolkata":"https://www.zomato.com/kolkata",
                    "chennai":"https://www.zomato.com/chennai",
                    "hyderabad":"https://www.zomato.com/hyderabad"
                },
                "BEAUTIFUL_SOUP":true,
                "SELECTORS":{
                    "WAIT":{
                        "TAG":"a",
                        "FIND_BY":"class",
                        "VALUE":["col-l-1by3","col-s-8","pbot0"]
                    }
                }
            },
            "RESTAURANT":{
                "BEAUTIFUL_SOUP": true,
                "SELECTORS":{
                    "WAIT":{
                        "TAG":"div",
                        "FIND_BY":"class",
                        "VALUE":["card","search-snippet-card","search-card"]
                    },
                    "NAME": {
                        "TAG":"a",
                        "FIND_BY":"class",
                        "VALUE":["result-title","hover_feedback","zred","bold","ln24","fontsize0"]
                    },
                    "IMAGE":{
                        "TAG":"a",
                        "FIND_BY":"class",
                        "VALUE":"feat-img lazy",
                        "ATTRIBUTE":"data-original"
                    },
                    "URL":{
                        "TAG":"a",
                        "FIND_BY":"data-class_name",
                        "VALUE":"o2_link",
                        "ATTRIBUTE":"href"
                    },
                    "TYPE":{
                        "TAG":"span",
                        "FIND_BY":"class",
                        "VALUE":["col-s-11","col-m-12","nowrap","pl0"]
                    },
                    "OPENS_AT":{
                        "TAG":"div",
                        "FIND_BY":"class",
                        "VALUE":"col-s-11 col-m-12 pl0 search-grid-right-text"
                    },
                    "STARS":{
                        "TAG":"div",
                        "FIND_BY":"class",
                        "VALUE":"ta-right floating search_result_rating col-s-4 clearfix"
                    },
                    "RATINGS":{
                        "TAG":"div",
                        "FIND_BY":"class",
                        "VALUE":"ta-right floating search_result_rating col-s-4 clearfix"
                    },
                    "NEXT_URL":{
                        "TAG":"a",
                        "FIND_BY":"class",
                        "VALUE":"next",
                        "ATTRIBUTE":"href"
                    }
                }
            },
            "DISH":{
                "BEAUTIFUL_SOUP": false,
                "SELECTORS":{
                    "WAIT":{
                        "FIND_BY":"class",
                        "VALUE":"menu-container"
                    },
                    "MENU_CONTAINER":{
                        "FIND_BY":"id",
                        "VALUE":"menu_ctg_"
                    },
                    "MENU_CONTAINER2":{
                        "FIND_BY":"id",
                        "VALUE":"menu_"
                    },
                    "CATEGORY_NAV":{
                        "FIND_BY":"class",
                        "VALUE":"o2_nav_bar"
                    },
                    "CATEGORIES":{
                        "FIND_BY":"class",
                        "VALUE":"item"
                    },
                    "CONTENT":{
                        "FIND_BY":"class",
                        "VALUE":"content"
                    },
                    "NAME":{
                        "FIND_BY":"class",
                        "VALUE":"header"
                    },
                    "PRICE":{
                        "FIND_BY":"class",
                        "VALUE":"description"
                    },
                    "IMAGE":{
                        "FIND_BY":"tag",
                        "VALUE":"img",
                        "ATTRIBUTE": "src"
                    },
                    "DESCRIPTION":{
                        "FIND_BY":"class",
                        "VALUE":"meta"
                    }
                }
            }
        }
    }
}
